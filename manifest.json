{
  "manifest_version": 2,
  "name": "Inbox Zero Coach",
  "description": "Inbox Zero Coach",
  "icons": {
    "128": "grey.png"
  },
  "version": "0.0.9",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inbox Zero Coach",
    "default_popup": "popup.html"
  }
}
